{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "properties": {
                        "alpine": "\uf300",
                        "arch": "\uf303",
                        "centos": "\uf304",
                        "debian": "\uf306",
                        "elementary": "\uf309",
                        "fedora": "\uf30a",
                        "gentoo": "\uf30d",
                        "linux": "\ue712",
                        "macos": "\ue711",
                        "manjaro": "\uf312",
                        "mint": "\uf30f",
                        "opensuse": "\uf314",
                        "raspbian": "\uf315",
                        "ubuntu": "\uf31c",
                        "windows": "\ue70f"
                    },
                    "leading_diamond": "\u256d\u2500",
                    "trailing_diamond": "\ue0b0",
                    "template": " {{ if .WSL }}\ue712 on {{ end }}{{ .Icon }} ",
                    "foreground": "#ffffff",
                    "background": "#cc2c44",
                    "type": "os",
                    "style": "diamond"
                },
                {
                    "template": " \uf489 {{ .Name }} ",
                    "foreground": "#512a53",
                    "background": "#c7f66f",
                    "type": "shell",
                    "style": "diamond"
                },
                {
                    "properties": {
                        "home_icon": "~",
                        "style": "short"
                    },
                    "template": " \ue5fe {{ .Path }} ",
                    "foreground": "#4c2d2d",
                    "powerline_symbol": "\ue0b0",
                    "background": "#51d7bc",
                    "type": "path",
                    "style": "powerline"
                },
                {
                    "properties": {
                        "branch_max_length": 25,
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "trailing_diamond": "\ue0b4",
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
                    "foreground": "#43CCEA",
                    "powerline_symbol": "\ue0b0",
                    "background": "#180b85",
                    "type": "git",
                    "style": "powerline",
                    "foreground_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
                        "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
                        "{{ if gt .Behind 0 }}#B388FF{{ end }}"
                    ]
                },
                {
                    "properties": {
                        "always_enabled": true
                    },
                    "trailing_diamond": "\ue0c6",
                    "template": " {{ if .Error }}\uf421 \uf119 {{ else }}\uf469 â™¥ {{ end }} ",
                    "foreground": "#474358",
                    "background": "#0f0",
                    "type": "status",
                    "style": "diamond",
                    "background_templates": [
                        "{{ if .Error }}#f00{{ end }}"
                    ]
                }
            ],
            "newline": true
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "properties": {
                        "command": "node -v | str replace 'v' ''",
                        "shell": "nu"
                    },
                    "template": "<#0055ff>\ued0d</> {{.Output}}<#cc7eda> | </>",
                    "foreground": "#0055ff",
                    "type": "command",
                    "style": "plain"
                },
                {
                    "template": "<#0055ff>\udb80\udf5b</> {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1000000000.0) }}GB",
                    "foreground": "#0055ff",
                    "type": "sysinfo",
                    "style": "diamond"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "template": "\u2570\u2500",
                    "foreground": "#254CF8",
                    "type": "text",
                    "style": "plain"
                }
            ],
            "newline": true
        },
        {
            "type": "rprompt",
            "alignment": "right",
            "segments": [
                {
                    "properties": {
                        "always_enabled": false,
                        "style": "austin",
                        "threshold": 150
                    },
                    "template": "\uf017 {{ .FormattedMs }} ",
                    "foreground": "#f00",
                    "type": "executiontime",
                    "style": "plain"
                },
                {
                    "properties": {
                        "command": "(date now | format date '%m\u6708%d\u65e5 %T') + ' \u5468' +  (['\u65e5' '\u4e00' '\u4e8c' '\u4e09' '\u56db' '\u4e94' '\u516d'] | get (date now | format date '%w' | into int))",
                        "shell": "nu"
                    },
                    "template": "\ueb7c {{.Output}}",
                    "foreground": "#ff00cc",
                    "type": "command",
                    "style": "plain"
                }
            ]
        }
    ],
    "version": 3,
    "final_space": true
}